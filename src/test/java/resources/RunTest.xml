<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite thread-count="2" name="Suite Login" parallel="tests">
    <listeners>
        <listener class-name="actions.commons.TestListener"/>
        <listener class-name="retryConfig.RetryListener"/>
    </listeners>
    <test name="Run on Chrome">
        <parameter name="browserName" value="chrome"/>
        <parameter name="environmentName" value="staging"/>
        <classes>
            <class name="testcases.LoginTestCase"/>
        </classes>
    </test>
    <test name="Run on EDGE">
        <parameter name="browserName" value="edge"/>
        <parameter name="environmentName" value="staging"/>
        <classes>
            <class name="testcases.LoginTestCase"/>
        </classes>
    </test>

</suite>

